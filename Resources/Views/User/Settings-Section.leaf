<section>
<h3 class="clearfix">#L10N("your-settings"){Your Settings} <a class="btn btn-link d-flex float-sm-right" href="/user/#(page.userID)/settings/edit#if(location){?p=#(location)}" title="#L10N("edit-settings"){Edit settings}"><span class="btn-label">#L10N("edit"){Edit}</span><svg class="feather"><use xlink:href="/icons/feather.svg#edit-2"/></svg></a></h3>
<div class="settings container-fluid keys-values">
#if(features.userNotifications.enabled) {
<div class="row"><div class="col-sm-6 key">#L10N("settings-notifications-services"){Notification Services}</div><div class="col-sm-6 value">#if(page.userSettings.notifications.emailEnabled || page.userSettings.notifications.pushoverEnabled){#if(page.userSettings.notifications.emailEnabled){#L10N("settings-notifications-email"){Email}<br/>}#if(page.userSettings.notifications.pushoverEnabled){#L10N("settings-notifications-pushover"){Pushover}</br>}}else{#L10N("none"){None}}#if(page.userSettings.notifications.emailEnabled || page.userSettings.notifications.pushoverEnabled){<div class="mt-2"><a href="/user/#(page.userID)/settings/notifications/test#if(location){?p=#(location)}" class="btn btn-secondary btn-sm" role="button">#L10N("settings-notifications-services-test"){Test Notification Services}</a></div>}</div></div>
</div>
}
</section>
